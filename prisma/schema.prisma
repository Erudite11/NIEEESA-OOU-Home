generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}


model Material {
  id            Int      @id @default(autoincrement())
  title         String
  level         String
  semester      String
  fileType      String
  url           String
  publicId      String?   
  originalName  String?
  createdAt     DateTime @default(now())
}